<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="autoNo">

    <insert id="saveComCode">
        INSERT INTO  AUTO_NO_MST
        (
             AUTO_NO
            ,AUTO_NM
            ,REF_CODE
            ,REF_NAME
        ) VALUES
        (
             #{autoNo}
            ,#{autoNm}
            ,#{refCode}
            ,#{refName}
        )
    </insert>

</mapper>