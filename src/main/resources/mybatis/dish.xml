<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dish">

    <insert id="insSub">
        INSERT INTO DISH_DTL_SUB
        (
             SUBTITLE_CODE
            ,SUBTITLE_NAME
        )
        VALUES
        (
         #{subtitleCode}
        ,#{subtitleName}
        )
    </insert>

    <insert id = "insDish">
        INSERT INTO DISH_INFO
        (
             DISH_CODE
            ,DISH_NAME
            ,DISH_CONTENTS
            ,DISH_PRICE
            ,DISH_G
            ,SUBTITLE_CODE
            ,DISH_VIEW_CNT
        )
        VALUES
        (
             #{dishCode}
            ,#{dishName}
            ,#{dishContents}
            ,#{dishPrice}
            ,#{dishG}
            ,#{subtileCode}
            ,0
        )
    </insert>
</mapper>